{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "sampledfdatafactory"
		},
		"AzureBlobStorage1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureBlobStorage1'"
		},
		"SqlServer1_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'SqlServer1'"
		},
		"blog_storage_for_data_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'blog_storage_for_data'"
		},
		"linked_service_for_sql_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'linked_service_for_sql'"
		},
		"HttpServer1_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://api.restful-api.dev/objects"
		},
		"SqlServer1_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "dataserver1221.database.windows.net"
		},
		"SqlServer1_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "datadb"
		},
		"SqlServer1_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "dhanush"
		},
		"linked_service_for_api_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://jsonplaceholder.typicode.com/users"
		},
		"linked_service_for_sql_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "database-server-for-assignment.database.windows.net"
		},
		"linked_service_for_sql_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "database-for-assignment"
		},
		"linked_service_for_sql_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "Dhanush"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "sampledfdatafactory_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureBlobStorage1_connectionString": {
						"value": "[parameters('AzureBlobStorage1_connectionString')]"
					},
					"SqlServer1_password": {
						"value": "[parameters('SqlServer1_password')]"
					},
					"blog_storage_for_data_connectionString": {
						"value": "[parameters('blog_storage_for_data_connectionString')]"
					},
					"linked_service_for_sql_password": {
						"value": "[parameters('linked_service_for_sql_password')]"
					},
					"HttpServer1_properties_typeProperties_url": {
						"value": "[parameters('HttpServer1_properties_typeProperties_url')]"
					},
					"SqlServer1_properties_typeProperties_server": {
						"value": "[parameters('SqlServer1_properties_typeProperties_server')]"
					},
					"SqlServer1_properties_typeProperties_database": {
						"value": "[parameters('SqlServer1_properties_typeProperties_database')]"
					},
					"SqlServer1_properties_typeProperties_userName": {
						"value": "[parameters('SqlServer1_properties_typeProperties_userName')]"
					},
					"linked_service_for_api_properties_typeProperties_url": {
						"value": "[parameters('linked_service_for_api_properties_typeProperties_url')]"
					},
					"linked_service_for_sql_properties_typeProperties_server": {
						"value": "[parameters('linked_service_for_sql_properties_typeProperties_server')]"
					},
					"linked_service_for_sql_properties_typeProperties_database": {
						"value": "[parameters('linked_service_for_sql_properties_typeProperties_database')]"
					},
					"linked_service_for_sql_properties_typeProperties_userName": {
						"value": "[parameters('linked_service_for_sql_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "sampledfdatafactory_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'sampledfdatafactory_ArmTemplate_0')]"
			]
		}
	]
}